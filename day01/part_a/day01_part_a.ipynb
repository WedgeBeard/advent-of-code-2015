    "import os\n",
    "\n",
    "filename = \"../input.txt\"\n",
    "with open(filename) as opened_file:\n",
    "    line = opened_file.readline()\n",
    "\n",
    "floor = 0\n",
    "for char in line:\n",
    "    if char == \"(\":\n",
    "        floor += 1\n",
    "    elif char == \")\":\n",
    "        floor -= 1\n",
    "\n",
    "print(floor)"
